#!/bin/bash
#Martin Notes
echo "quelle est ta note ?"
read var1
if [ $var1 -ge "18" ]
then
echo "Bravo !"
elif [ $var1 -eq "12" ]
then
echo "c'est acceptable mais tu peux faire mieux"
elif [ $var1 -ge "13" ] && [ $var1 -le "17" ]
then
echo "c'est une bonne note"


elif [ $var1 -lt "12" ]
then 
echo "il faut r√©viser plus !"
fi
